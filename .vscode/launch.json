  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "learn breakout",
      "type": "debugpy",
      "request": "launch",
      "program": "src/baselines/breakout.py",
      "console": "integratedTerminal",
      "args": []
    },
    {
      "name": "play breakout",
      "type": "debugpy",
      "request": "launch",
      "program": "src/baselines/breakout.py",
      "console": "integratedTerminal",
      "args": [
        "play",
      ]
    },
  ]
}
